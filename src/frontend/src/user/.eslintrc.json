{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
        "requireConfigFile": false
    },
    "rules": {
        "react/jsx-no-useless-fragment": "error",
        "react/react-in-jsx-scope": "off"
    }
}